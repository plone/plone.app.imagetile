<?xml version="1.0"?>
<registry>

    <record name="plone.app.deco.tiles">
        <value purge="false">
            <element>plone.app.deco.tile_image</element>
        </value>
    </record>
    <records prefix="plone.app.deco.tile_image"
             interface="plone.app.deco.interfaces.ITile">
        <value key="name">image</value>
        <value key="label">Image</value>
        <value key="icon">++resource++plone.app.imagetile/icon.png</value>
    </records>

</registry>
